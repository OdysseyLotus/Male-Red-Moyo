<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<!-- Red Moyo -->
	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="Alien_RedMoyo"]/alienRace/generalSettings/alienPartGenerator/useGenderedHeads</xpath>
		<value>
			<useGenderedHeads>true</useGenderedHeads>
		</value>
	</Operation>
	
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="Alien_RedMoyo"]/alienRace/generalSettings/alienPartGenerator/alienbodytypes</xpath>
		<value>
			<li>Male</li>
		</value>
	</Operation>
	
