<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<!-- Red Moyo -->
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="Alien_RedMoyo"]/alienRace/generalSettings/alienPartGenerator/bodyTypes</xpath>
		<value>
			<li>Male</li>
		</value>
	</Operation>
	
	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="Alien_RedMoyo"]/alienRace/generalSettings/alienPartGenerator/headTypes</xpath>
		<value>
			<headTypes>
				<li>Female_MoyoHead</li>
				<li>Female_MoyoHead1</li>
				<li>Female_MoyoHead2</li>
				<li>Female_MoyoHead3</li>
				<li>Female_MoyoHead4</li>
				<li>Female_MoyoHead5</li>
				
				<li>Male_MoyoHead</li>
				<li>Male_MoyoHead1</li>
				<li>Male_MoyoHead2</li>
				<li>Male_MoyoHead3</li>
				<li>Male_MoyoHead4</li>
				<li>Male_MoyoHead5</li>
			</headTypes>
		</value>
	</Operation>
	
	<!-- Body Addon Offsets -->
